{
  "name": "set-comprobantes-backend",
  "version": "1.0.0",
  "description": "Servicio de automatizacion de comprobantes SET Paraguay - multitenant",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "dev:worker": "ts-node-dev --respawn --transpile-only src/worker.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "start:worker": "node dist/worker.js",
    "migrate": "ts-node src/db/migrate.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.995.0",
    "@aws-sdk/s3-request-presigner": "^3.995.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^6.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@types/luxon": "^3.7.1",
    "@types/pdfkit": "^0.17.5",
    "@xmldom/xmldom": "^0.8.11",
    "axios": "^1.6.8",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "facturacionelectronicapy-kude": "^1.0.18",
    "facturacionelectronicapy-qrgen": "^1.0.9",
    "facturacionelectronicapy-setapi": "^1.0.34",
    "facturacionelectronicapy-xmlgen": "^1.0.277",
    "facturacionelectronicapy-xmlsign": "^1.0.28",
    "fastify": "^4.26.2",
    "iconv-lite": "^0.7.2",
    "luxon": "^3.7.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "puppeteer": "^22.6.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-user-preferences": "^2.4.1",
    "solvecaptcha-javascript": "^1.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.2",
    "@types/xmldom": "^0.1.34",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3"
  }
}
